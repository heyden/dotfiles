# System
alias sshgh='eval $(ssh-agent -s) && ssh-add ~/.ssh/github_id_rsa'

# Git
alias gits='git status -sb'
alias gpl='git pull'
alias gplm='git pull origin master'
alias gp='git push'
alias gpum='git push origin master'

